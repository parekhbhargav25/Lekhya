generator client {
  provider   = "prisma-client-js"
  engineType = "binary"
}

datasource db {
  provider = "postgresql"
}

model Receipt {
  id               String   @id @default(cuid())
  userId           String
  s3Key            String
  s3Url            String
  status           String   @default("uploaded")
  extractedJson    Json?
  createdAt        DateTime @default(now())
  categoryOverride String?
  user             User     @relation(fields: [userId], references: [id], onDelete: Cascade)
}

model User {
  id        String    @id @default(cuid())
  email     String    @unique
  name      String?
  createdAt DateTime  @default(now())
  password  String?
  receipts  Receipt[]
}
